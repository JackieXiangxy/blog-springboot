<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jackie.blogweb.mapper.AccessMapper">
    <resultMap id="BaseResultMap" type="com.jackie.blogweb.model.Access">

        <id column="id" jdbcType="VARCHAR" property="id"/>
        <result column="ipaddr" jdbcType="VARCHAR" property="ipaddr"/>
        <result column="addr" jdbcType="VARCHAR" property="addr"/>
        <result column="view_time" jdbcType="TIMESTAMP" property="viewTime"/>
    </resultMap>
    <sql id="Base_Column_List">

        id, ipaddr, addr, view_time
    </sql>


    <!--    添加网站访问地址-->
    <insert id="insertSelective" parameterType="com.jackie.blogweb.model.Access">

        insert into access
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="ipaddr != null">
                ipaddr,
            </if>
            <if test="addr != null">
                addr,
            </if>
            <if test="viewTime != null">
                view_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id,jdbcType=VARCHAR},
            </if>
            <if test="ipaddr != null">
                #{ipaddr,jdbcType=VARCHAR},
            </if>
            <if test="addr != null">
                #{addr,jdbcType=VARCHAR},
            </if>
            <if test="viewTime != null">
                #{view_time,jdbcType=TIMESTAMP},
            </if>
        </trim>
    </insert>

</mapper>